<?xml version="1.0"?>
<configuration xmlns="https://tony19.github.io/logback-android/xml">
    <appender name="stdout" class="com.esri.logger.khronicle.appender.TestAppender">
        <encoder>
            <pattern>%message</pattern>
        </encoder>
    </appender>

    <root level="TRACE">
        <appender-ref ref="stdout" />
    </root>
</configuration>
